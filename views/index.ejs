<!DOCTYPE html>
<html>
<head>
	<title>Pharmacies List</title>
</head>
<body>
	<h1>Pharmacies List</h1>
	<ul>
		<% pharmacys.forEach((pharmacy) => { %>
			<li>
				<a href="/pharmacy/<%= pharmacy._id %>"><%= pharmacy.name %></a>
			</li>
		<% }); %>
	</ul>
</body>
</html>